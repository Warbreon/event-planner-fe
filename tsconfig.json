/* Visit https://aka.ms/tsconfig to read more about this file */
{
	"include": ["src/**/*"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "es2016",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
